# yt-cipher Environment Configuration
# Copy this file to .env and modify the values as needed

# Server Configuration
SERVER_PORT=3000
SERVER_HOST=0.0.0.0

# Authentication (IMPORTANT: Change this from the default!)
API_TOKEN=YOUR_API_TOKEN

# Worker Configuration
WORKER_CONCURRENCY=16
WORKER_TASK_TIMEOUT=60000
WORKER_MAX_RETRIES=5

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=999999999

# Logging
LOG_LEVEL=warn
LOG_FORMAT=text

# Cache Configuration
PLAYER_CACHE_SIZE=10000
PLAYER_CACHE_TTL=7200000
PLAYER_CACHE_CLEANUP_INTERVAL=600000

SOLVER_CACHE_SIZE=5000
SOLVER_CACHE_TTL=7200000
SOLVER_CACHE_CLEANUP_INTERVAL=600000

PREPROCESSED_CACHE_SIZE=15000
PREPROCESSED_CACHE_TTL=14400000
PREPROCESSED_CACHE_CLEANUP_INTERVAL=600000

STS_CACHE_SIZE=10000
STS_CACHE_TTL=3600000
STS_CACHE_CLEANUP_INTERVAL=600000